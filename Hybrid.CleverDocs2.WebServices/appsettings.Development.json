{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information",
      "Hybrid.CleverDocs2": "Debug",
      "System.Net.Http.HttpClient": "Information"
    }
  },
  
  // Development Database Configuration
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Port=5433;Database=cleverdocs;Username=admin;Password=MiaPassword123;Include Error Detail=true"
  },
  
  // Development Redis Configuration
  "Redis": {
    "Configuration": "localhost:6380,password=dev_redis_pass,abortConnect=false",
    "InstanceName": "CleverDocs2Dev",
    "DefaultDatabase": 1,
    "ConnectTimeout": 10000,
    "SyncTimeout": 10000
  },
  
  // Development RabbitMQ Configuration
  "RabbitMQ": {
    "Host": "localhost",
    "Port": 5674,
    "VirtualHost": "/dev",
    "Username": "dev_rabbitmq_user",
    "Password": "dev_rabbitmq_pass",
    "ManagementPort": 15674,
    "Heartbeat": 30,
    "RequestedConnectionTimeout": 15000
  },
  
  // Development R2R API Configuration
  "R2R": {
    "ApiUrl": "http://localhost:7272",
    "ConfigPath": "/configs/dev.toml",
    "DefaultTimeout": 60,
    "MaxRetries": 5,
    "ApiKey": "dev_api_key",
    "RetryDelayMs": 500,
    "CircuitBreakerThreshold": 3,
    "CircuitBreakerDurationMs": 15000
  },
  
  // Development Health Checks Configuration
  "HealthChecks": {
    "Enabled": true,
    "TimeoutSeconds": 30,
    "Postgres": {
      "Enabled": true,
      "TimeoutSeconds": 10
    },
    "Redis": {
      "Enabled": true,
      "TimeoutSeconds": 10
    },
    "RabbitMQ": {
      "Enabled": false,
      "TimeoutSeconds": 10
    },
    "R2R": {
      "Enabled": true,
      "TimeoutSeconds": 30
    }
  },
  
  // Development JWT Configuration
  "Jwt": {
    "SecretKey": "development-jwt-secret-key-for-testing-purposes-only-32-chars-minimum",
    "Issuer": "Hybrid.CleverDocs2.Development",
    "Audience": "Hybrid.CleverDocs2.DevClient",
    "ExpirationMinutes": 120,
    "RefreshTokenExpirationDays": 30
  },
  
  // Development CORS Configuration
  "Cors": {
    "AllowedOrigins": ["*"],
    "AllowedMethods": ["*"],
    "AllowedHeaders": ["*"],
    "AllowCredentials": true
  },
  
  // Development File Upload Configuration
  "FileUpload": {
    "MaxFileSizeBytes": 209715200,
    "AllowedExtensions": [".pdf", ".docx", ".txt", ".md", ".html", ".json", ".csv", ".xlsx", ".pptx", ".rtf"],
    "TempDirectory": "./temp/dev",
    "StorageDirectory": "./storage/dev"
  },
  
  // Development Background Services Configuration
  "BackgroundServices": {
    "IngestionWorker": {
      "Enabled": true,
      "IntervalSeconds": 10,
      "BatchSize": 5
    },
    "MaintenanceWorker": {
      "Enabled": false,
      "IntervalHours": 1,
      "CleanupTempFiles": true,
      "CleanupOldLogs": false
    }
  },
  
  // Development Monitoring Configuration
  "Monitoring": {
    "Metrics": {
      "Enabled": true,
      "Endpoint": "/dev-metrics"
    },
    "Tracing": {
      "Enabled": true,
      "ServiceName": "Hybrid.CleverDocs2.Development"
    }
  }
}
