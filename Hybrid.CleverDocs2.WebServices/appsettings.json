{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Postgres": "Host=localhost;Database=mydb;Username=myuser;Password=mypassword"
  },
  "Redis": {
    "Configuration": "localhost:6380,abortConnect=false"
  },
  "RabbitMQ": {
    "Host": "localhost",
    "VirtualHost": "/",
    "Username": "user",
    "Password": "pass"
  },
  "R2R": {
    "ApiUrl": "http://localhost:7272",
    "ConfigPath": "/configs/full.toml",
    "DefaultTimeout": 30,
    "MaxRetries": 3
  }
}
