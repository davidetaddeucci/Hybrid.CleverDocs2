{
  "files.exclude": {
    "**/Thumbs.db": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/node_modules": true,
    "**/*.log": true
  },
  "files.autoSave": "off",
  "typescript.disableAutomaticTypeAcquisition": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules/**": true,
    "**/.git/subtree-cache/**": true
  },
  "search.exclude": {
    "**/.git": true,
    "**/node_modules": true,
    "**/bower_components": true
  }
}
